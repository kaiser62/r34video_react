{
  "version": 2,
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/wsgi.py"
    }
  ],
  "env": {
    "USE_PROXY": "false",
    "DEBUG_MODE": "false",
    "MAX_WORKERS": "1",
    "CACHE_SIZE": "32",
    "REQUEST_TIMEOUT": "10",
    "FLASK_ENV": "production",
    "PYTHONUNBUFFERED": "1",
    "PYTHONDONTWRITEBYTECODE": "1"
  },
  "functions": {
    "wsgi.py": {
      "maxDuration": 30
    }
  }
}
